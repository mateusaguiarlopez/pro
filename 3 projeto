// Online C compiler to run C program online
#include <stdio.h>
int main() {
int idade;
printf ("digite sua senha: ");
scanf ("%d", &idade);
if(idade >=18){
  printf("\nisso é uma festa, divirta-se");
    }else if(idade >=12){
  printf("\nvá beber uma fanta uva na barraquinha!");
   }else{
  printf("\nbeba chá e camomila!");
   }
  
  
  printf("\na sua idade é %d", idade);
 if(idade >=65){
    printf("\npasse na farmácia!");
 }
printf("\nvá para casa dormir!");
 
 return 0;
}
